{
    "name": "app",
    "description": "",
    "license": "UNLICENSED",
    "version": "1.0.0",
    "dependencies": {
        "@ethossoftworks/interactor": "./ethossoftworks-interactor-1.0.0.tgz",
        "@ethossoftworks/interactor-react": "./ethossoftworks-interactor-react-1.0.0.tgz",
        "@ethossoftworks/job": "^1.0.3",
        "@ethossoftworks/outcome": "^2.1.1",
        "lodash.isequal": "^4.5.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-transition-group": "^4.4.1",
        "rxjs": "^6.6.3"
    },
    "devDependencies": {
        "@ethossoftworks/knock-on-wood": "^2.0.1",
        "@types/lodash.isequal": "^4.5.5",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-transition-group": "^4.4.0",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^5.0.1",
        "css-minimizer-webpack-plugin": "^1.2.0",
        "mini-css-extract-plugin": "^1.3.4",
        "prettier": "^2.1.2",
        "sass": "^1.32.5",
        "sass-loader": "^10.1.1",
        "source-map-explorer": "^2.5.2",
        "source-map-loader": "^2.0.0",
        "typescript": "^4.1.2",
        "webpack": "^5.16.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.2"
    },
    "scripts": {
        "format": "npx prettier --write --loglevel warn src/**/*.ts*",
        "ts-compile": "npx tsc -b src/tsconfig.json",
        "ts-compile-test": "npx tsc -b src/tsconfig.test.json",
        "build": "yarn format && yarn ts-compile && npx webpack --env prod",
        "build-dev": "yarn format && yarn ts-compile && npx webpack --env dev",
        "build-test": "yarn format && yarn ts-compile-test && npx webpack --env test",
        "start-bundler": "npx webpack serve --env dev",
        "start-ts": "npx tsc -b src/ -w",
        "bundle-inspect": "yarn build && npx source-map-explorer ./build/dist/app.main.js --no-border-checks --only-mapped",
        "bundle-inspect-vendor": "yarn build && npx source-map-explorer ./build/dist/app.vendor.js --no-border-checks --only-mapped",
        "test": "yarn build-test && node build/dist/app.test.js",
        "test-inspect": "yarn build-test && node --inspect-brk build/dist/app.test.js",
        "update-local-lib": "node scripts/update-local-lib.js"
    },
    "prettier": {
        "endOfLine": "lf",
        "tabWidth": 4,
        "semi": false,
        "printWidth": 120,
        "useTabs": false
    }
}